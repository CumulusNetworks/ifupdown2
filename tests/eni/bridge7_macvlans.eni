auto lo
iface lo inet loopback

auto eth0
iface eth0 inet dhcp
    vrf mgmt

auto mgmt
iface mgmt
    vrf-table auto

auto bridge
iface bridge
      bridge-vlan-aware yes
      bridge-ports swp_AA_
      bridge-stp on
      bridge-vids 1-33
      bridge-pvid 192

auto bridge.1
iface bridge.1
      address-virtual 42:38:39:FF:00:1 10.7.192.1/1

